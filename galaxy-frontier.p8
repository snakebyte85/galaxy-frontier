pico-8 cartridge // http://www.pico-8.com
version 29
__lua__
#include util.lua
#include constants.lua
#include projectiles.lua
#include player.lua
#include space.lua

global={
   step=0,
   debug=false
}

function _init()
   global.step=0
   space_init()
   player_init()
   true_btnp_init()
end

function _update60()
   true_btnp_update()
   space_update()
   player_update()
   projectiles_update()
   global.step = global.step + 1
end

function _draw()
   cls()
   if global.debug then
      rect(0,0,const.screen.max_x-1,const.screen.max_y-1, const.colors.red)
   end
   space_draw()
   player_draw()
   projectiles_draw()
end

